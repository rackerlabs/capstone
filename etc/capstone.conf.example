[rackspace]
base_url = rackspace_base_url  # e.g. https://..../v2.0
feed_url = https://example.com/events
# Polling period in seconds to request new feeds events
polling_period = 60
#TODO(jorge.munoz): Move cache invalidator properties to their own group
marker_file=/var/run/capstone_invalidator/marker
[service_admin]
id = id_here
username = username_here
password = password_here
project_id = project_id_here

[capstone]
caching = true
cache_time = 60
